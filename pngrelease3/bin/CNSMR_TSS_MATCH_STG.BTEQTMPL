UPDATE TRILLIUM_OUTPUT1
FROM 
(
SEL HSHLD_PRSNA_ID
FROM HSHLD_PRSNA
GROUP BY 1
) B
SET HH_FLAG = 'Y'
WHERE CAST(TRILLIUM_OUTPUT1.REFERENCE_HOUSEHOLD_ID AS INTEGER)=B.HSHLD_PRSNA_ID
;
.IF ERRORLEVEL > 0 THEN .GOTO EXIT;

UPDATE TRILLIUM_OUTPUT1
SET LE_FLAG = 'N'
WHERE LE_FLAG IS NULL
;
.IF ERRORLEVEL > 0 THEN .GOTO EXIT;

UPDATE TRILLIUM_OUTPUT1 
SET HH_FLAG = 'N'
WHERE HH_FLAG IS NULL
;
.IF ERRORLEVEL > 0 THEN .GOTO EXIT;

COLLECT STATS ON $DATABASENAME.TRILLIUM_OUTPUT1
COLUMN MKTNG_PGM_NBR;
.IF ERRORLEVEL > 0 THEN .GOTO EXIT;
