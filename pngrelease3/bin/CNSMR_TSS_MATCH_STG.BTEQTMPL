,STG.ACNLGMT_DATE
,STG.CNTCT_OPTN_NBR
,STG.CNTCT_OPTN_IND
,STG.CNSMR_CHCE_DATETM
,STG.PREFR_IND
,CASE WHEN TOUT.PR_REV_GROUP > 0
