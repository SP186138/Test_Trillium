               <ARGUMENTS>
                  <FILE_QUALIFIER>OUTPUT</FILE_QUALIFIER>
                  <DATA_FILE_NAME>stdout</DATA_FILE_NAME>
                  <DDL_FILE_NAME>$(DDL)/at_tranfrmr_p1.ddx</DDL_FILE_NAME>




<SELECT_RECORDS/>
</ARGUMENTS>
            </OUTPUT_SETTINGS>
            
            <TABLE_RECODING>
               <ARGUMENTS>
<ENTRY>
<ENTRY_ID>1</ENTRY_ID>
<ENTRY_DESCRIPTION>Find Masks of Aptno and remove from end of Address 1</ENTRY_DESCRIPTION>
<FILE_QUALIFIER>RECODE1</FILE_QUALIFIER>
<TABLE_FILE_NAME>$(USRTABLES)/Apt_mask_table.csv</TABLE_FILE_NAME>
<DDL_FILE_NAME>$(USRTABLES)/Apt_Mask_table.ddx</DDL_FILE_NAME>
<FILE_DELIMITER>Comma</FILE_DELIMITER>
<USE_QUOTES_AS_QUALIFIER>Y</USE_QUOTES_AS_QUALIFIER>
<LOOKUP_PHRASE_SEPARATOR>"" ""</LOOKUP_PHRASE_SEPARATOR>
<LOOKUP_PHRASE_CHANGE/>
<LOOKUP_POSITION>E</LOOKUP_POSITION>








<LOOKUP_TABLE_FIELDS>Mask_Lookup</LOOKUP_TABLE_FIELDS>
<LOOKUP_FIELDS_CASE_SENSITIVE>N</LOOKUP_FIELDS_CASE_SENSITIVE>
<LOOKUP_OUTPUT_FIELDS>ADDR_LINE_1_TXT</LOOKUP_OUTPUT_FIELDS>
<LOOKUP_OUTPUT_FIELDS_FORMAT>L</LOOKUP_OUTPUT_FIELDS_FORMAT>
<RECODE_TABLE_FIELDS>Mask_Recode</RECODE_TABLE_FIELDS>
<RECODE_TABLE_FIELDS_FORMAT>L</RECODE_TABLE_FIELDS_FORMAT>
<RECODE_OUTPUT_FIELDS>ADDR_LINE_1_TXT</RECODE_OUTPUT_FIELDS>

</ENTRY>
</ARGUMENTS>
            </TABLE_RECODING>
            <CONDITIONALS>
               <ARGUMENTS>
                  <ENTRY>
                     <ENTRY_ID>1</ENTRY_ID>
                     <FOR_FILES>OUTPUT</FOR_FILES>
                     <CONDITION>IF (ALWAYS)
        IF (ALWAYS)
    ENDIF
    IF (ADDR_LINE_1_TXT != SPACES AND APT_NUM != SPACES)
    SET FOUND_APT = OCCSTR(TRIM( ADDR_LINE_1_TXT) , TRIM( APT_NUM) , "L")
    IF (FOUND_APT &gt; "-1")
    SET APT_NUM = SPACES
    SET FOUND_APT = SPACES
    ENDIF
    ENDIF
    IF (ADDR_LINE_1_TXT = SPACES AND STRET_NAME != SPACES AND APT_NUM != SPACES)
    SET FOUND_APT = OCCSTR(TRIM( STRET_NAME) , TRIM( APT_NUM) , "L")
    IF (FOUND_APT &gt; "-1")
    SET APT_NUM = SPACES
    ENDIF
    ENDIF
    
    ENDIF</CONDITION>
                  </ENTRY>
               </ARGUMENTS>
            </CONDITIONALS>
         </PARAMETER>
      </OUTPUT>
      <PROCESS>
         <PARAMETER>
            <PROCESS_SETTINGS>
